 "html": "<table width=\"100%\">\r\n    <thead>\r\n    <tr>\r\n        <td width=\"100%\">\r\n           <!-- <div class=\"header\">\r\n                {% if letter_head %}\r\n                <img src=\"/files/mu-master-header.png\">\r\n                {% endif %}\r\n            </div>\r\n            <div class=\"header-space\"></div>-->\r\n        </td>\r\n    </tr>\r\n    </thead>\r\n    <tbody>\r\n    <tr>\r\n        <td>\r\n            <div class=\"content\">\r\n                <div class=\"col-xs-12\">\r\n                    <div class=\"row section-break\">\r\n                        <div class=\"col-xs-6\">\r\n                            <div class=\"row\">\r\n                                <div class=\"col-xs-6 pr-0\">\r\n                                    <img src=\"/files/Interio-Floors-logo-1.jpg\" style=\"width: 100%!important; height: 115px;\">\r\n                                </div>\r\n                                <div class=\"col-xs-6\">\r\n                                    <p><b>\r\n                                        Interio Floors<br>\r\n                                        Old Muslim Town<br>\r\n                                        Lahore, 54000</b>\r\n                                    </p>\r\n                                    <p><b>\r\n                                        042-35912838<br>\r\n                                        042-35912839</b>\r\n                                    </p>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-xs-6\">\r\n                            <h4 class=\"text-right\" style=\"font-size: 20px;\">\r\n                                QUOTATION\r\n                            </h4>\r\n                            <table class=\"table table-bordered\">\r\n                                <tr>\r\n                                    <td width=\"33.33%\">Reference #</td>\r\n                                    <td width=\"33.33%\"><b>Date</b></td>\r\n                                    <td width=\"33.33%\"><b>Quotation #</b></td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <td></td>\r\n                                    <td>{{doc.get_formatted(\"transaction_date\") or \"\"}}</td>\r\n                                    <td>{{doc.name or \"\" }}</td>\r\n                                </tr>\r\n                            </table>\r\n                        </div>\r\n                        <div class=\"col-xs-12\">\r\n                            <div class=\"invoice-top-div\">\r\n                                <div class=\"row\">\r\n                                    <div class=\"col-md-12\">\r\n                                        <table class=\"table table-bordered\">\r\n                                            <tr>\r\n                                                <td rowspan=\"2\">\r\n                                                   <b>Customer Name :</b>\r\n                                                </td>\r\n                                                <td rowspan=\"2\">\r\n                                                    {{ doc.customer_name or \"\" }}<br>\r\n                                                    {{ doc.address_display or \"\" }}\r\n                                                </td>\r\n                                                <td>\r\n                                                    Customer Phone\r\n                                                </td>\r\n                                                <td>\r\n                                                    Delivery To\r\n                                                </td>\r\n                                            </tr>\r\n                                            <tr>\r\n                                                <td>{{ doc.contact_mobile or \"\"}}</td>\r\n                                                <td>\r\n                                                    {{doc.shipping_address or \"\"}}\r\n                                                </td>\r\n                                            </tr>\r\n\r\n                                        </table>\r\n                                    </div>\r\n                                </div>\r\n                        </div>\r\n                            <div class=\"invoice-table-div\">\r\n\r\n                            <table class=\"table table-bordered\" width=100% align=\"center\">\r\n                                <thead id=\"thead\">\r\n                                <tr>\r\n                                    <th width=\"12%\" class=\"text-center\" > <b>Item Code</b></th>\r\n                                    <th width=\"35%\" class=\"text-center\"> <b>Description</b></th>\r\n                                    <th width=\"15%\" class=\"text-center\" > <b>Quantity</b></th>\r\n                                    <th width=\"8%\" class=\"text-center\" > <b>U/M</b></th>\r\n                                    <th width=\"15%\" class=\"text-center\" > <b>Price / Unit</b></th>\r\n                                    <th width=\"15%\" class=\"text-center\" > <b>Amount</b></th>\r\n                                </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                {% for row in doc.items %}    \r\n                                <tr>\r\n                                    <td class=\"text-center\">{{row.item_code}}</td>\r\n                                    <td>{{row.description}}</td>\r\n                                    <td class=\"text-center\">{{row.qty|int}}</td>\r\n                                    <td class=\"text-center\">{{row.uom}}</td>\r\n                                    <td class=\"text-center\">{{row.get_formatted(\"rate\")}}</td>\r\n                                    <td class=\"text-center\">{{row.get_formatted(\"amount\")}}</td>\r\n                                </tr>\r\n                                {% endfor %}\r\n                                <tr>\r\n                                    <td colspan=\"3\"></td>\r\n                                    <td style=\"background-color: #c0c0c0;\" colspan=\"3\">\r\n                                        <div class=\"row\">\r\n                                            <div class=\"col-xs-4\">\r\n                                                <h4>Total</h4>\r\n                                            </div>\r\n                                            <div class=\"col-xs-8 text-right\">\r\n                                                <h4>{{doc.get_formatted(\"grand_total\")}}</h4>\r\n                                            </div>\r\n                                        </div>\r\n                                    </td>\r\n                                </tr>\r\n                                </tbody>\r\n                            </table>\r\n                        </div>\r\n                            <div class=\"bottom-details\">\r\n                            <div class=\"row\">\r\n                                <div class=\"col-xs-4 text-center\">\r\n                                    <h4>\r\n                                        <span style=\"border-top: 1px solid #000000;width: 150px\">\r\n                                            Customer Signature\r\n                                        </span>\r\n                                    </h4>\r\n                                </div>\r\n                                <div class=\"col-xs-4\">\r\n                                    <h4 class=\"text-center\">\r\n                                        <span style=\"border-top: 1px solid #000000;width: 150px\">\r\n                                            Approved By\r\n                                        </span>\r\n                                    </h4>\r\n                                </div>\r\n                                <div class=\"col-xs-4\">\r\n                                    <h4 class=\"text-center\">\r\n                                        <span style=\"border-top: 1px solid #000000;width: 150px\">\r\n                                            Issued By\r\n                                        </span>\r\n                                    </h4>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </td>\r\n    </tr>\r\n    </tbody>\r\n    <tfoot>\r\n    <tr>\r\n        <td>\r\n\r\n        </td>\r\n    </tr>\r\n    </tfoot>\r\n</table>",
